# MongoDB connection string (required)
MONGODB_URI=mongodb+srv://<user>:<password>@cluster.mongodb.net/mediflow

# Comma-separated list of 10-digit phone numbers that are allowed to call admin endpoints.
# Example: ADMIN_PHONES=9876543210,9123456789
ADMIN_PHONES=

# Port the server listens on (optional, defaults to 3000)
PORT=3000
